var removeOuterParentheses = function(S) {
let str =''
let c=0
let fin=''
S=S.split("")
  
       for(let i of S){
        if(i=="("){
            c++
        if(c>1){
            str+="("
        }}else{
            c--
            if(c==0){
                fin+=str
                str=''
            }else{
                str+=")"
            }
        }
           
       }
        
      
           
    
    return fin
    };
