/**
 * Definition for singly-linked list.
 * function ListNode(val, next) {
 *     this.val = (val===undefined ? 0 : val)
 *     this.next = (next===undefined ? null : next)
 * }
 */
/**
 * @param {ListNode} head
 * @return {number}
 */
var getDecimalValue = function(head) {
    let curr= head
    console.log(head)
    let cc=[]
    while(curr!=null){
         cc.push(curr.val)
       
        curr=curr.next
    
        
    }
 
    return parseInt(cc.join(""),2)
};
